<section id="catalog">
  <h1>All Movies</h1>
  <div class="band">

    {{#each movies}}
    <div class="flip flip-vertical">
      <div class="front">
        <img src="{{this.imageUrl}}" alt="{{this.title}}" />
      </div>
      <div class="back">
        <h1>{{this.title}}</h1>
        <h2>Genre: {{this.genre}}</h2>
        <p>Description: {{this.description}}</p>
        <a href="#" class="details">Details</a>
      </div>
    </div>
    {{/each}}
  </div>
</section>